                s1.src='https://embed.tawk.to/5cc5caf74aa6620df2cf942b/default';
                break;
            case 'ptbr':
            case 'pt-BR':
                s1.src='https://embed.tawk.to/5cc5cb12d6e05b735b44aa1f/default';
                break;
            case 'ro':
            case 'ro-RO':
