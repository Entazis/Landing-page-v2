    Tawk_API.onLoad = function(){
        Tawk_API.setAttributes({
            'name': decodeURIComponent(getCookie('name')),
            'email': decodeURIComponent(getCookie('email'))
            //'email': getCookie('email')
        }, function (error) {});
    };
</script>
